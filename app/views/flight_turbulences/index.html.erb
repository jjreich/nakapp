<p id="notice"><%= notice %></p>

<h1>Flight Turbulences</h1>

<table>
  <thead>
    <tr>
      <th>Turbulencetype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @flight_turbulences.each do |flight_turbulence| %>
      <tr>
        <td><%= flight_turbulence.turbulenceType %></td>
        <td><%= link_to 'Show', flight_turbulence %></td>
        <td><%= link_to 'Edit', edit_flight_turbulence_path(flight_turbulence) %></td>
        <td><%= link_to 'Destroy', flight_turbulence, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Flight Turbulence', new_flight_turbulence_path %>
